{
	"name": "blindern/intern",
	"description": "Blindern Studenterhjem Intern",
	"keywords": ["Blindern Studenterhjem", "Laravel"],
	"license": "MIT",
	"type": "project",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:henrist/openldapauth.git"
		}
	],
	"require": {
		"laravel/framework": "5.1.31",
		"nesbot/carbon": "1.21.0",
		"twitter/bootstrap": "3.0.3",
		"eluceo/ical": "0.10.0",
		"nategood/httpful": "0.2.20",
		"jenssegers/mongodb": "2.3.5",
		"google/apiclient": "1.1.7",
		"johngrogg/ics-parser": "1.0.3"
	},
	"require-dev": {
		"phpunit/phpunit": "4.8.24",
		"phpspec/phpspec": "2.4.1"
	},
	"autoload": {
		"classmap": [
			"database",
			"app/Models"
		],
		"psr-4": {
			"App\\": "app/",
			"Blindern\\Intern\\": "app/src"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		],
		"test": [
			"phpunit"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
