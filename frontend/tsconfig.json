{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "strictNullChecks": true,
    "baseUrl": "./src"/*,
    "paths": {
      "*": [
        "src/*",
        "node_modules/*"
      ]
    }*/
  },
  "include": ["src/**/*"]
}
